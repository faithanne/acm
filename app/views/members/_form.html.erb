<%= simple_form_for(@member) do |f| %>

  <%= f.input :name %>
  <%= f.input :phone %>
  <%= f.input :major %>
  <%= f.input :email %>
  <%= f.input :fees_paid %>
  <%= f.input :campus_resident %>
  <%= f.input :volunteer %>
  <%= f.input :position %>
  <%= f.input :referrer %>
  <%= f.association :recruiter %>
  <%= f.input :interests %>
  <%= f.button :submit %>

<% end %>
