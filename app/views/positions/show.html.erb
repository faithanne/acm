<% if user_signed_in? %>

  <p id="notice"><%= notice %></p>

  <p>
    <b>Title:</b>
    <%= @position.title %>
  </p>


  <%= link_to 'Edit', edit_position_path(@position) %> |
  <%= link_to 'Back', positions_path %>

<% else %>

  <p>Please <%= link_to 'Sign In', new_user_session_path %>!</p>

<% end %>
